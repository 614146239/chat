{
  "name": "camere",
  "version": "1.0.8",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "614146239",
  "homepage": "https://github.com/614146239/electron-camere",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview ",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config --publish=always",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@mediapipe/face_mesh": "^0.4.1633559619",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@tensorflow-models/body-segmentation": "^1.0.2",
    "@tensorflow-models/face-landmarks-detection": "^1.0.5",
    "@tensorflow/tfjs": "^4.20.0",
    "axios": "^1.7.2",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.2.1",
    "element-plus": "^2.7.6",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "postcss-px-to-viewport": "^1.1.1",
    "qs": "^6.12.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^2.5.0",
    "three": "^0.165.0",
    "vite-plugin-mediapipe": "^0.1.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.3.2",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "20.14.8",
    "@types/three": "^0.165.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "less": "^4.2.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.3.1",
    "vue": "^3.4.30",
    "vue-tsc": "^2.0.22"
  },
  "build": {
    "appId": "https://github.com/614146239/qq",
    "win": {
      "target": "nsis",
      "icon": "./resources/camera.png"
    },
    "directories": {
      "buildResources": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
